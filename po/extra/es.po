#: data/settings.desktop.in:4 data/settings.appdata.xml.in:9
msgid "System Settings"
msgstr "Configuración del Sistema"

#: data/settings.desktop.in:5 data/settings.appdata.xml.in:10
msgid "Change system and user settings"
msgstr "Cambie opciones del sistema y de usuario"

#: data/settings.desktop.in:11
msgid "Center;Control;Panel;Preferences;System;"
msgstr "Centro;Control;Panel;Preferencias;Sistema;"

#: data/settings.appdata.xml.in:12
msgid ""
"A system settings application that focuses on modularity and simplicity."
msgstr ""
"Una aplicación de configuración del sistema centrada en modularidad y "
"simplicidad."

#: data/settings.appdata.xml.in:35
msgid "elementary, Inc."
msgstr "elementary, Inc."

#: data/settings.appdata.xml.in:44
msgid "Screen reader improvements"
msgstr "Se implementaron mejoras para lectores de pantalla"

#: data/settings.appdata.xml.in:45 data/settings.appdata.xml.in:61
#: data/settings.appdata.xml.in:77 data/settings.appdata.xml.in:85
#: data/settings.appdata.xml.in:97
msgid "Updated translations"
msgstr "Traducciones actualizadas"

#: data/settings.appdata.xml.in:52
msgid "App Updates:"
msgstr "Actualizaciones de la aplicación:"

#: data/settings.appdata.xml.in:54
msgid "New redesigned app icon"
msgstr "Ahora se usa un nuevo yrediseñado icono de aplicación"

#: data/settings.appdata.xml.in:55
msgid "New flatter and more responsive design"
msgstr "Un nuevo diseño plano y más responsivo"

#: data/settings.appdata.xml.in:56
msgid "Smoother animations"
msgstr "Animaciones más suaves"

#: data/settings.appdata.xml.in:57
msgid "Search results are more relevant and more clearly formatted"
msgstr ""
"Los resultados de búsqueda son más relevantes y con un formato más claro"

#: data/settings.appdata.xml.in:58
msgid "Use Portals when available"
msgstr "Se usan portales cuando sea posible"

#: data/settings.appdata.xml.in:59
msgid "Back Buttons have a secondary-click menu for faster navigation"
msgstr ""
"Los botones de «atrás» ahora tienen un menú haciendo clic con el botón "
"secundario para mejor navegación"

#: data/settings.appdata.xml.in:60
msgid "Window now has a reasonable minimum size"
msgstr "La ventana ahora tiene un tamaño mínimo más razonable"

#: data/settings.appdata.xml.in:76
msgid "Bundle app icon"
msgstr "Ahora el icono está empaquetado con la misma aplicación"

#: data/settings.appdata.xml.in:93
msgid "Add multitouch navigation gestures"
msgstr "Se agregaron gestos de navegación multitoque"

#: data/settings.appdata.xml.in:94
msgid "Better support smaller displays"
msgstr "Mejor soporte para pantallas más pequeñas"

#: data/settings.appdata.xml.in:95
msgid "Remove old GNOME Control Center compatibility layer"
msgstr ""
"Se eliminó la antigua capa de compatibilidad del Centro de Control de GNOME"

#: data/settings.appdata.xml.in:96
msgid "Stability and performance improvements"
msgstr "Mejoras de estabilidad y rendimiento"

#~ msgid "Redesign search results view"
#~ msgstr "Rediseño de la vista de resultados de búsqueda"

#~ msgid "Tab key moves focus out of search"
#~ msgstr "La tecla Tab traslada el foco fuera de la búsqueda"

#~ msgid "Fix an issue that prevented tiling on small displays"
#~ msgstr ""
#~ "Se corrige un problema que impedía el ajuste de las ventanas en mosaico "
#~ "en pantallas pequeñas"

#~ msgid "Fix an issue with restoring the correct window size"
#~ msgstr ""
#~ "Corregido un problema con la restauración del tamaño de ventana correcto"

#~ msgid "Make the category grid fill the available space when maximized"
#~ msgstr ""
#~ "Hacer que la cuadrícula de categorías llene el espacio disponible cuando "
#~ "se maximiza"

#~ msgid ""
#~ "Pressing the \"Down\" arrow key from the search box correctly focuses the "
#~ "first row"
#~ msgstr ""
#~ "Al presionar la tecla de flecha \"Abajo\" del cuadro de búsqueda, se "
#~ "enfoca correctamente la primera fila"

#~ msgid "Drop dynamic items from the Dock secondary-click menu"
#~ msgstr ""
#~ "Soltar elementos dinámicos desde el menú de clic secundario del Dock"

